
@{
    Layout = "~/Views/Shared/_EssamTest.cshtml";
}




    <h1>Test1</h1>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">FName</th>
                <th scope="col">LName</th>
                <th scope="col">Salary</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Employees)
            {
                <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.FName</td>
                    <td>@item.LName</td>
                    <td>@item.Salar</td>
                </tr>

            }

        </tbody>
    </table>
